{
	"name": "paywall-unblocker",
	"description" : "unblocks articles to read without subscription",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
	    "service_worker": "background.js"
	 },
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage", "activeTab", "scripting"
	],
	"options_page": "options.html"
}